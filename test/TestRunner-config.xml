<?xml version="1.0" encoding="UTF-8"?><!--flex-config-->
<flex-config>

	<benchmark>false</benchmark>
	<default-background-color>#FF0000</default-background-color>
	<use-network>true</use-network>
	<static-link-runtime-shared-libraries>true</static-link-runtime-shared-libraries>

	<compiler>

		<incremental>true</incremental>
		<optimize>true</optimize>
		<keep-generated-actionscript>false</keep-generated-actionscript>
		<warn-no-constructor>false</warn-no-constructor>
		<warn-no-explicit-super-call-in-constructor>true</warn-no-explicit-super-call-in-constructor>
		<show-actionscript-warnings>true</show-actionscript-warnings>
		<show-binding-warnings>true</show-binding-warnings>
		<show-unused-type-selector-warnings>true</show-unused-type-selector-warnings>
		<debug>true</debug> 
		<verbose-stacktraces>true</verbose-stacktraces>
		<headless-server>true</headless-server>
		
		<!-- Enable trace statement omission. -->
		<!-- <omit-trace-statements>false</omit-trace-statements> -->

		<!-- for swc's -->
		<library-path append="true">
			<path-element>../lib/</path-element>
		</library-path>
		
	  <keep-as3-metadata append="true">
			<name>Inject</name>
		</keep-as3-metadata>
		
		<source-path append="true">
			<path-element>../src/</path-element>
	    </source-path> 
		
		<!--		
		<source-path append="true">
			<path-element>../lib/src/</path-element>
		</source-path>
		-->
		
		<fonts>
			<languages>
				<language-range>
					<lang>Basic Latin</lang>
					<range>U+0020-U+002F,U+0030-U+0039,U+003A-U+0040,U+0041-U+005A,U+005B-U+0060,U+0061-U+007A,U+007B-U+007E,U+00A3,U+00A5,U+0024,U+20AC,U+00A9,U+2022</range>
				</language-range>
			</languages>
		</fonts>

	</compiler>

	<metadata>
		<title>Hacksaw Test Runner Application</title>
		<description>Testing a log</description>
		<publisher>Made in TextMate</publisher>
		<creator>Simon Gregory</creator>
		<language>EN</language>
	</metadata>
	
</flex-config>